QPushButton {
    background-color: #4CAF50;
    color: white;
    border: none;
    border-radius: 5px;
    padding: 5px 10px;
    font-size: 14px;
}

QPushButton:hover {
    background-color: #45a049;
}

QPushButton:pressed {
    background-color: #3e8e41;
}

QLabel {
    font-size: 16px;
    color: #333333;
}

QFrame#info_widget {
    background-color: #fff8dc;
    border: 1px solid #cccccc;
    border-radius: 10px;
}