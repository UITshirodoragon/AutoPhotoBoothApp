QMainWindow {
    background-color: #f0f0f0;
}

QFrame#main_frame {
    background-color: #ffffff;
    border: 2px solid #cccccc;
    border-radius: 10px;
}

QWidget#container {
    background-color: #ffffff;
    border: 1px solid #cccccc;
    border-radius: 5px;
}

QWidget#widget_0 {
    background-color: lightblue;
    border: 1px solid black;
    border-radius: 5px;
}

